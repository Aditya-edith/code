class Solution {
public:
    int climbStairs(int n) {
        
        int res;
        
        if(n<=3)return n;
        int prev=3;
        int uska_prev = 2;
        
        for(int i=4;i<=n;i++){
            
            res = prev + uska_prev;
            uska_prev = prev;
            prev = res;
        }
        return res;
        
    }
};
